{
  "name": "Shades",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Wikipedia Dankifier",
  "homepage_url": "https://github.com/revan/shades",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://*.wikipedia.org/wiki/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.wikipedia.org/wiki/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [	  
        "https://*.wikipedia.org/wiki/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/tracking-min.js",
        "js/eye-min.js",
        "src/inject/inject.js"
      ]
    }
  ],
  "web_accessible_resources": ["shades.png"]
}
